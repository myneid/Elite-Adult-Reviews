<% unless @sites.nil? %>
  <% if @sites.type.to_s == 'PagingEnumerator'  %>
    <% if @sites.size > 0 %>
      <%= render :partial => "layouts/reviewlist", :locals => { :section => "Search Results For #{params[:query]}", :sites => @sites, :is_top => 1 } %>
        <br>Page: <%= paginating_links(@sites, {:params => {:query => params[:query]}} ) %>
      <% end %>
    <% end %>
  <% end %>
